{
    "manifest_version": 2,
    "author": "explodingcamera",
    "name": "MusiqPlus",
    "short_name": "musiq+",
    "description": "Enhance you MUSIQPLUS experience!",
    "homepage_url": "https://explodingcamera.com",
    "version": "0.1.0",

    "icons": {
        "32": "32.png",
        "64": "64.png",
        "128": "128.png"
